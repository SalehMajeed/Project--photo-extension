{
    "manifest_version": 2,
    "name": "Photo Extension",
    "version": "0.001",
    "web_accessible_resources": [
        "*.jpeg"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "photo.js"
            ]
        }
    ]
}